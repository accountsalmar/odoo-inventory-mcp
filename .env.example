# Odoo Connection Configuration
# Copy this file to .env and update with your Odoo credentials

# Odoo server URL (without trailing slash)
ODOO_URL=https://duracubeonline.com.au

# Odoo database name
ODOO_DB=live

# Odoo username (usually email)
ODOO_USERNAME=accounting@qagroup.com.au

# Odoo API Key (generate from Odoo user preferences)
ODOO_API_KEY=your_api_key_here
